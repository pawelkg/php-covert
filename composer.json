{
  "name": "stephenlake/php-covert",
  "description": "Process background PHP tasks.",
  "keywords": ["process", "background task", "nohup", "pid"],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.2"
  },
  "suggest": {
    "ext-posix": "Needed while runing on *nix platform"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "Covert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Covert\\Tests\\": "tests/"
    }
  }
}